{
    "name": "cerealed",
    "description": "Binary serialisation library for D",
    "homepage": "https://github.com/atilaneves/cerealed",
    "authors": ["Atila Neves"],
    "license": "BSD 3-clause",
    "targetType": "library",
    "targetPath": "bin",
    "configurations": [
        {"name": "library"},
        {"name": "unittest",
         "targetType": "executable",
         "sourcePaths": ["tests"],
         "preBuildCommands": ["dub fetch dtest && dub run dtest -- -Isrc -f bin/ut.d"],
         "mainSourceFile": "bin/ut.d",
         "dependencies": {
             "unit-threaded": "~>0.5.3"
         }
        }
    ]
}
