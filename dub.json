{
    "name": "cerealed",
    "description": "Binary serialisation library for D",
    "homepage": "https://github.com/atilaneves/cerealed",
    "authors": ["Atila Neves"],
    "license": "BSD 3-clause",
    "targetType": "library",
    "targetPath": "bin",
    "dependencies": {
        "concepts": "~>0.0.6"
    },
    "configurations": [
        {"name": "library"},
        {
            "name": "unittest",
            "targetType": "executable",
            "targetName": "ut",
            "sourcePaths": ["tests"],
            "preBuildCommands": ["dub run unit-threaded -c gen_ut_main -- -f bin/ut.d"],
            "mainSourceFile": "bin/ut.d",
            "dependencies": {
                "unit-threaded": "~>0.7.0"
            }
        },
        {
            "name": "ut",
            "targetType": "executable",
            "targetName": "utl",
            "sourcePaths": ["tests"],
            "preBuildCommands": ["dub run unit-threaded -c gen_ut_main -- -f bin/ut.d"],
            "mainSourceFile": "bin/ut.d",
            "dependencies": {
                "unit-threaded": "~>0.7.0"
            },
            "versions": ["unitThreadedLight"]
        }
    ]
}
